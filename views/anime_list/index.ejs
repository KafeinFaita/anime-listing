<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>LIST</h1>
    <table>
        <thead>
            <th>Anime Title</th>
            <th>Status</th>
            <th>Score</th>
            <th>Action</th>
        </thead>
<% list.forEach(li => { %>
        <tr>
            <td><a href="/anime/<%= li.mal_id %> "><%= li.anime_title %></a></td>
            <td><%= li.status_name %></td>
            <td><%= li.score %></td>
            <td><p>TEST</p></td>
        </tr>
<% }) %> 
    </table>
</body>
